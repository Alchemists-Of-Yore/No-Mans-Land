{
    "multipart": [
    {
      "apply": {
        "model": "nomansland:block/plants/frosted_grass_snowy",
        "uvlock": true
      },
      "when": {
        "snowy": "true"
      }
    },
    {
      "apply": {
        "model": "nomansland:block/plants/frosted_grass",
        "uvlock": true
      },
      "when": {
        "snowy": "false"
      }
    },
    {
      "apply": {
        "model": "nomansland:block/plants/frosted_grass_snow_layer",
        "uvlock": true
      },
      "when": {
        "snowy": "true"
      }
    }
  ]
}